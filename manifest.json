{
    "name":"Clip Board & Notes",
    "version":"0.1.2",
    "description":"Sticky notes in browser",
    "manifest_version":2,
    "permissions":["storage","declarativeContent","activeTab","clipboardRead","clipboardWrite"],
    "background":{
        "scripts":["background.js"],
        "persistent":true
    },
    "browser_action":{
        "default_popup":"popup.html"
    },
    "options_page":"options/options.html"
}
